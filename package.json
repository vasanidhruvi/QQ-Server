    "babel-eslint": "^10.1.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "dotenv-safe": "^8.2.0",
    "ejs": "^3.1.9",
    "expeditious-engine-redis": "^0.1.2",
    "express": "^4.18.2",
    "express-expeditious": "^5.1.1",
    "express-validator": "^6.15.0",
    "helmet": "^4.6.0",
    "i18n": "^0.13.4",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.1.0",
    "mongoose": "^5.13.20",
    "mongoose-paginate-v2": "^1.7.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.5",
    "nodemailer-mailgun-transport": "^2.1.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "request-ip": "^2.2.0",
    "sjcl": "^1.0.7",
    "trim-request": "^1.0.6",
    "uuid": "^8.3.2",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "chai": "^4.3.8",
    "chai-http": "^4.4.0",
